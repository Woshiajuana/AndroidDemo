<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment2">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.owulia.wowcool.ui.fragment.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_demoSlideMenuFragment"
            app:destination="@id/demoSlideMenuFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_demoWEqualsHFragment"
            app:destination="@id/DemoWEqualsHFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_demoIconFontFragment"
            app:destination="@id/demoIconFontFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
        <action
            android:id="@+id/action_mainFragment_to_demoTabBarFragment"
            app:destination="@id/demoTabBarFragment"
            app:enterAnim="@anim/anim_slide_in_right"
            app:exitAnim="@anim/anim_slide_out_left"
            app:popEnterAnim="@anim/anim_slide_in_left"
            app:popExitAnim="@anim/anim_slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/demoSlideMenuFragment"
        android:name="com.owulia.wowcool.ui.fragment.demo.DemoSlideMenuFragment"
        android:label="fragment_demo_slide_menu"
        tools:layout="@layout/fragment_demo_slide_menu" />
    <fragment
        android:id="@+id/DemoWEqualsHFragment"
        android:name="com.owulia.wowcool.ui.fragment.demo.DemoWEqualsHFragment"
        android:label="fragment_wow_w_equals_h"
        tools:layout="@layout/fragment_demo_w_equals_h" />
    <fragment
        android:id="@+id/demoIconFontFragment"
        android:name="com.owulia.wowcool.ui.fragment.demo.DemoIconFontFragment"
        android:label="fragment_demo_icon_font"
        tools:layout="@layout/fragment_demo_icon_font" />
    <fragment
        android:id="@+id/demoTabBarFragment"
        android:name="com.owulia.wowcool.ui.fragment.demo.DemoTabBarFragment"
        android:label="fragment_demo_tab_bar"
        tools:layout="@layout/fragment_demo_tab_bar" />
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.owulia.wowcool.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>